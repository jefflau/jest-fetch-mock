{
  "name": "jest-fetch-mock",
  "version": "2.1.2",
  "description": "fetch mock for jest",
  "main": "src/index.js",
  "types": "types",
  "scripts": {
    "test": "jest && npm run tsc && npm run dtslint",
    "dtslint": "dtslint types",
    "tsc": "tsc"
