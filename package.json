    "url": "git+https://github.com/jefflau/jest-fetch-mock.git"
  },
  "keywords": [
    "jest",
    "mock",
    "fetch"
  ],
  "author": "Jeff Lau <jeff-lau@live.com> (http://jefflau.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefflau/jest-fetch-mock/issues"
  },
  "homepage": "https://github.com/jefflau/jest-fetch-mock#readme",
  "dependencies": {
    "cross-fetch": "^2.2.2",
    "promise-polyfill": "^7.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.10",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "dtslint": "^0.3.0",
    "jest": "^23.5.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.12.1",
    "typescript": "^3.2.1"
  },
  "prettier": {
    "semi": false,
    "editor.formatOnSave": true,
    "singleQuote": true,
    "overrides": [
